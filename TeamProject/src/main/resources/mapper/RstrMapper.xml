<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.team.mapper.RstrMapper">
	<select id="getList" resultType="com.example.team.dto.RstrVO">
		select * from rstr
	</select>
	
	<select id="getRstr">
		select * from rstr where rstr_nm = #{rstr_nm}
	</select>

</mapper>